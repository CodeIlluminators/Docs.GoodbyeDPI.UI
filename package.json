{
	"name": "faq_goodbyedpi_ui",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"next:dev": "next dev",
		"next:lint": "next lint",
		"commit": "cz",
		"export": "next export",
		"deploy": "next build && next export && gh-pages -d out",
		"precommit": "lint-staged",
		"prepare": "husky install",
		"build": "next build",
		"start:local": "next start -p 3001",
		"start:debug": "next start --debug --watch",
		"start:dev": "next start --watch",
		"start": "next start",
		"sort-imports:list": "bunx import-sort --list-different '**/*.{ts,js}'",
		"sort-imports:write": "bunx import-sort --write '**/*.{ts,js}'",
		"format:check": "prettier . --check",
		"format:write": "prettier . --write",
		"lint:check": "mkdir -p logs && eslint . --no-fix --output-file logs/eslint-check.log",
		"lint:fix": "eslint . --fix",
		"clean:check": "run-p sort-imports:check format:check lint:check",
		"clean:write": "bun sort-imports:write && bun format:write"
	},
	"lint-staged": {
		"*": [
			"bun run clean:write"
		]
	},
	"husky": {
		"hooks": {
			"post-commit": "sh .husky/post-commit.sh"
		}
	},
	"license": "Apache-2.0",
	"authors": [
