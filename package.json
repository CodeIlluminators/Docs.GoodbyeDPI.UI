		"@types/node": "^20.17.5",
		"@vercel/analytics": "^1.3.2",
		"@vercel/speed-insights": "^1.0.14",
		"class-transformer": "^0.5.1",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
